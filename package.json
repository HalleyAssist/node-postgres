{
  "name": "pg",
  "version": "8.6.0",
  "description": "PostgreSQL client - pure javascript & libpq with the same API",
  "keywords": [
    "database",
    "libpq",
    "pg",
    "postgre",
    "postgres",
    "postgresql",
    "rdbms"
  ],
  "scripts": {
    "test": "yarn lerna exec yarn test",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "docs:build": "cd docs && yarn build",
    "docs:start": "cd docs && yarn start",
    "pretest": "yarn build",
    "prepublish": "yarn build",
    "lint": "eslint --cache 'packages/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "lerna": "^3.19.0",
    "prettier": "3.0.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "buffer-writer": "2.0.0",
    "packet-reader": "1.0.0",    
    "pg-connection-string": "^2.7.0",
    "pg-pool": "^3.7.0",
    "pg-protocol": "./packages/pg-protocol",
    "pg-types": "^2.1.0",
    "pgpass": "1.x"
  },
  "peerDependencies": {
    "pg-native": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "pg-native": {
      "optional": true
    }
  },
  "scripts": {
    "preinstall": "cd ./packages/pg-protocol && npm build",
    "test": "make test-all"
  },
  "files": [
    "packages"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "./packages/pg/lib"
}
